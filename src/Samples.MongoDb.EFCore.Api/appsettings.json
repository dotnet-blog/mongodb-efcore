{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "D:\\Work\\GitHub\\dotnet-blog\\mongodb-efcore\\src\\Samples.MongoDb.EFCore.Api\\.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{CorrelationId}] [{Level}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{CorrelationId}] [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MediaLibraryDatabase": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "media_library"
  },
  "ConnectionStrings": {
    "RedisDb": "localhost:6379",
    "RabbitMq": "rabbitmq://rabbitmq:rabbitmq@localhost:5672/sample.api"
  },
  "MovieInfoService": {
    "Url": "https://www.omdbapi.com/",
    "ApiKey": ""
  },
  "MoviesInfoCheckJob": {
    "Schedule": "0 0 * ? * *"
  }
}
